{
    "name": "lee-to/moonshine-tree-resource",
    "description": "Tree resource for moonshine",
    "keywords": ["moonshine"],
    "type": "library",
    "homepage": "https://moonshine-laravel.com",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/lee-to/moonshine-tree-resource/issues",
        "source": "https://github.com/lee-to/moonshine-tree-resource"
    },
    "authors": [
        {
            "name": "Danil Shutsky",
            "email": "thecutcode@gmail.com",
            "homepage": "https://cutcode.dev"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Leeto\\MoonShineTree\\": "src/"
        }
    },
    "conflict": {
        "moonshine/moonshine": "<2.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leeto\\MoonShineTree\\Providers\\MoonShineTreeServiceProvider"
            ]
        }
    }
}
